@model IEnumerable<Db_store.Models.Account>
@{
    ViewBag.Title = "About";
}
<table class="table table-hover">
    <tr>
        <th>Name</th>
        <th>Email</th>
        <th>Account Type</th>
        <th>Balance</th>
    </tr>
    @{
        foreach (var account in Model)
        {
            <tr>
                <td>@account.User.UserName</td>
                <td>@account.User.UserEmail</td>
                <td>@account.AccountType</td>
                <td>@account.Balance</td>
            </tr>    
        }
    }
</table>
